<?xml version="1.0" encoding="utf-8"?>
<odoo>

      <!-- specific list view -->
      <record model="ir.ui.view" id="vehicle_view_tree">
        <field name="name">fleet.vehicle.tree</field>
        <field name="model">fleet.vehicle</field>
        <field name="priority" eval="16"/>
        <field name="arch" type="xml">
          <tree>
            <field name="name" />
          </tree>
        </field>
      </record>

    <!-- action view -->
    <record model="ir.actions.act_window" id="vehicle_list_action">
      <field name="name">Vehicles</field>
      <field name="res_model">fleet.vehicle</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="FleetFlow" id="fleetflow_menu_root" web_icon="fleet_flow,static/description/icon.png">
      <menuitem id="fleetflow_menu_1" name="Fleet" sequence="1">
        <menuitem
          id="fleetflow_vehicle_menu"
          name="Vehicles"
          action="vehicle_list_action"
          sequence="10"/>
      </menuitem>
    </menuitem>

</odoo>
