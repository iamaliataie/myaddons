<?xml version="1.0" encoding="utf-8"?>
<templates>
   <t t-name="button_van.KanbanView.Buttons" t-inherit="web.KanbanView.Buttons">
       <xpath expr="//*[@class='btn btn-primary o-kanban-button-new']" position="after">
           <button type="button" class="btn btn-primary" style="margin-left: 10px;"  t-on-click="loadProducts">
               Load Products
           </button>
           <button type="button" class="btn btn-primary" style="margin-left: 10px;"  t-on-click="unloadProducts">
               Unload Products
           </button>
           <button type="button" class="btn btn-primary" style="margin-left: 10px;"  t-on-click="loadTransfers">
               Transfers
           </button>
       </xpath>
   </t>
</templates>